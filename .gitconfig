[user]
        name = Volkan Ozkan
	email = vozkan@bol.com
[credential]
	helper = osxkeychain	
[alias]
	st = status
	lol = log --graph --oneline --all --date-order
	d = !"git diff-index --quiet HEAD -- || clear; git --no-pager diff --patch-with-stat"
	co = checkout
	cob = checkout -b
	cl = clone
	p = pull
	f = fetch
	update = pull --rebase origin master
        back = reset HEAD~
        authors = shortlog --summary --numbered
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold # line info
	old = red # deletions
	new = green # additions
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
